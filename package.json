{
  "name": "advanced-angular-bootstrap-components",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build-scrolling-tabs": "ng build scrolling-tabs",
    "lint-scrolling-tabs": "ng lint scrolling-tabs",
    "test-scrolling-tabs": "ng test scrolling-tabs --watch=false",
    "publish-scrolling-tabs": "npm run build-scrolling-tabs && cd dist/scrolling-tabs && npm publish",
    "build-search-input": "ng build search-input",
    "lint-search-input": "ng lint search-input",
    "test-search-input": "ng test search-input --watch=false",
    "publish-search-input": "npm run build-search-input && cd dist/search-input && npm publish",
    "build-pager": "ng build pager",
    "lint-pager": "ng lint pager",
    "test-pager": "ng test pager --watch=false",
    "publish-pager": "npm run build-pager && cd dist/pager && npm publish",
    "start": "npm run build-libs && ng serve -o --port 4500",
    "build-libs": "npm run build-scrolling-tabs && npm run build-search-input && npm run build-pager",
    "test": "npm run test-scrolling-tabs && npm run test-search-input && npm run test-pager",
    "lint": "npm run lint-scrolling-tabs && npm run lint-search-input && npm run lint-input"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "bootstrap": "4.4.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.3.17",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^10.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
